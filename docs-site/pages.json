	"easycom": {
		"autoscan": true,
		"custom": {
			"^bl-collapse-panel$": "@/../uni_modules/belay-unix/components/bl-collapse/panel.uvue",
			"^bl-tab-panel$": "@/../uni_modules/belay-unix/components/bl-tab-panel/index.uvue",
			"^bl-timeline-item$": "@/../uni_modules/belay-unix/components/bl-timeline/item.uvue",
			"^bl-step$": "@/../uni_modules/belay-unix/components/bl-step/index.uvue",
			"^bl-grid-item$": "@/../uni_modules/belay-unix/components/bl-grid-item/index.uvue",
