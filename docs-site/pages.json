{
	"pages": [
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "Belay-Unix 组件库",
				"navigationStyle": "custom"
			}
		},
		{
			"path": "pages/components/index",
			"style": {
				"navigationBarTitleText": "组件索引",
				"navigationStyle": "custom"
			}
		},
		{
			"path": "pages/component-detail/index",
			"style": {
				"navigationBarTitleText": "组件详情",
				"navigationStyle": "custom"
			}
		},
		{
			"path": "pages/guide/index",
			"style": {
				"navigationBarTitleText": "使用指南"
			}
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "Belay-Unix 组件库",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
