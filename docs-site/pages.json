			"^bl-menu-item$": "@/../uni_modules/belay-unix/components/bl-menu/item.uvue",
			"^bl-tree-node$": "@/../uni_modules/belay-unix/components/bl-tree/node.uvue",
			"^bl-col$": "@/../uni_modules/belay-unix/components/bl-col/index.uvue",
